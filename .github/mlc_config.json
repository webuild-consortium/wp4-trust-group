{
  "ignorePatterns": [
    { "pattern": "^mailto:" },
    { "pattern": "^#" }
  ],
  "httpHeaders": [
    {
      "urls": [
        "https://github.com/",
        "https://guides.github.com/",
        "https://help.github.com/",
        "https://docs.github.com/"
      ],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "aliveStatusCodes": [200, 206, 301, 302]

}
